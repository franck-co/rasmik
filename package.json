{
  "name": "rasmik",
  "version": "0.0.10",
  "description": "Routes And Services for MikroORM",
  "author": "franck-co",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsc",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsc"
  },
  "peerDependencies": {
    "@mikro-orm/core": ">=5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es6"
  },
  "module": "dist/rasmik.esm.js",
  "size-limit": [
    {
      "path": "dist/rasmik.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/rasmik.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@mikro-orm/core": "^5.0.2",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.3",
    "@types/mkdirp": "^1.0.2",
    "@types/pluralize": "^0.0.29",
    "husky": "^7.0.4",
    "size-limit": "^7.0.3",
    "ts-morph": "^13.0.2",
    "tslib": "^2.3.1",
    "typescript": "^4.5.2"
  },
  "dependencies": {
    "@mikro-orm/migrations": "^5.0.2",
    "@mikro-orm/mysql": "^5.0.2",
    "@mikro-orm/reflection": "^5.0.2",
    "mkdirp": "^1.0.4",
    "pluralize": "^8.0.0"
  },
  "keywords": [
    "mikro-orm",
    "typescript",
    "client",
    "generator"
  ]
}
